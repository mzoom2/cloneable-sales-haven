
# Frontend Environment Variables
# Base URL for API requests (no trailing slash)
VITE_API_BASE_URL=http://localhost:5000/api

# Backend Environment Variables
# Host for the backend server
BACKEND_HOST=0.0.0.0
# Port for the backend server
BACKEND_PORT=5000
# Comma-separated list of allowed origins for CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,https://your-frontend-domain.com
# Database URL
DATABASE_URL=sqlite:///store_data.db
# Secret key for session
SESSION_SECRET=your_secure_secret_key_here
